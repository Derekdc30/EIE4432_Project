{
  "builds": [
    {
    "src": "src/index.js",
    "use": "@vercel/node"
    },
    {
    "src": "static/**",
    "use": "@vercel/static"
    }
    ],
  "routes": [
    { "src": "/auth/login", "dest": "/api/route.js" },
    { "src": "/auth/logout", "dest": "/api/route.js" },
    { "src": "/auth/me", "dest": "/api/route.js" },
    { "src": "/auth/register", "dest": "/api/route.js" },
    { "src": "/auth/pay/visa", "dest": "/api/route.js" },
    { "src": "/auth/pay/paypal", "dest": "/api/route.js" },
    { "src": "/auth/pay/AE", "dest": "/api/route.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}